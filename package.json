{
  "name": "my-react-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^6.2.0",
    "@emotion/react": "^11.4.0",
    "@emotion/styled": "^11.3.0",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/qs": "^6.9.7",
    "@types/react-dom": "^17.0.0",
    "antd": "^4.16.10",
    "craco-less": "^1.18.0",
    "dayjs": "^1.10.6",
    "eslint-config-prettier": "^8.3.0",
    "history": "^5.0.0",
    "jira-dev-tool": "^1.6.59",
    "qs": "^6.10.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-query": "^3.19.2",
    "react-router": "^6.0.0-beta.0",
    "react-router-dom": "^6.0.0-beta.0",
    "react-scripts": "4.0.3",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "scripts": {
    "start": "craco start",
    "build": "craco build",
    "test": "craco test",
    "predeploy": "npm run build",
    "deploy":"gh-pages -d build -r git@github.com:K1SuMi/K1SuMi.github.io.git",
    "eject": "react-scripts eject",
    "prepare": "husky install",
    "commit": "git add -A && git-cz",
    "mock": "json-server --c json-server.json ./__json_server_mock__/db.json --middlewares ./__json_server_mock__/middleware.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest",
      "prettier"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.4",
    "@commitlint/config-conventional": "^12.1.4",
    "@types/react": "^17.0.14",
    "@welldone-software/why-did-you-render": "^6.2.0",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^7.0.1",
    "json-server": "^0.16.3",
    "lint-staged": "^11.1.0",
    "prettier": "2.3.2"
  },
  "lint-staged": {
    "*.{js,jsx,css,md,ts,tsx}": "prettier --write"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "msw": {
    "workerDirectory": "public"
  }
}
